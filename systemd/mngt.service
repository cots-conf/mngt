[Unit]
Description=Web application server for management of COTS conference

[Service]
EnvironmentFile=/home/root/projects/mngt/.env
WorkingDirectory=/home/root/projects/mngt/
ExecStart=/home/root/.pyenv/versions/3.10.0/bin/gunicorn -b localhost:5000 -w mngt.wsgi:app

[Install]
WantedBy=multi-user.target
